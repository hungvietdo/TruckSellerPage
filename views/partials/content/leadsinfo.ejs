    <!-- Search -->
    <header>
        <div class="container">
            <div class="row">    
                <div class="col-lg-6 col-lg-offset-3">
                    <br> <br><br><br>
                </div>
            </div>
            <table class="table table-hover">
                    <thead>
                      <tr>
                        <th>Lead Id</th>  
                        <th>Ad Id</th>
                        <th>Create Date</th>
                        <th>Buyer Name</th>
                        <th>Buyer Email</th>
                        <th>Buyer Phone</th>
                        <th>Message</th>
                      </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0; i<data.length; i++) { %>
                            <tr>
                                <td><%- data[i]._id%></td>    
                                <td><%- data[i].adId%></td>
                                <td><%- data[i].date%></td>    
                                <td><%- data[i].name%></td>
                                <td><%- data[i].email%></td>    
                                <td><%- data[i].phone%></td>
                                <td><%- data[i].message%></td>
                            </tr>
                        <%}%>
                    </tbody>
                  </table>
                </div>
        </div>
        <br><br><br><br><br><br>
    </header>